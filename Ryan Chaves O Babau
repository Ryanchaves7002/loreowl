<script>
    // Lista de livros (pode ser expandida ou carregada de um banco de dados no futuro)
    const books = [
        { title: "Orgulho e Preconceito", link: "https://th.bing.com/th?id=OPHS.OK%2brlzjjFEhyYg474C474&w=248&h=248&o=5&pid=21.1" },
        { title: "O Morro dos Ventos Uivantes", link: "./o-morro-dos-ventos-uivantes.html" },
        { title: "P.S. Eu Te Amo", link: "./ps-eu-te-amo.html" },
        { title: "Como Eu Era Antes de Voc√™", link: "./como-eu-era-antes.html" },
        { title: "Um Dia", link: "./um-dia.html" },
        { title: "Amor Nos Tempos do C√≥lera", link: "./amor-nos-tempos.html" },
        { title: "O Di√°rio de uma Paix√£o", link: "./o-diario-de-uma-paixao.html" },
        { title:"Volume 16 Dragon Ball", link:"https://http2.mlstatic.com/D_NQ_NP_987320-MLU77344751946_072024-O.webp"},
        { title:"Biblia sagrada",link :"https://www.mercadolivre.com.br/biblia-rcm-trinitariana-acf-letra-gigante-editora-sbtb/p/MLB20808066"},
        {title:"A arte da guerra", link:"./aaed2f0c9de630f0e83c1a4da57bd3b5.webp"}
    ];

    const searchBox = document.getElementById('searchBox');
    const suggestions = document.getElementById('suggestions');

    // Fun√ß√£o para atualizar as sugest√µes
    searchBox.addEventListener('input', () => {
        const query = searchBox.value.toLowerCase();
        suggestions.innerHTML = ''; // Limpa as sugest√µes anteriores

        if (query) {
            const filteredBooks = books.filter(book => book.title.toLowerCase().includes(query));
            if (filteredBooks.length > 0) {
                filteredBooks.forEach(book => {
                    const suggestion = document.createElement('a');
                    suggestion.href = book.link;
                    suggestion.textContent = book.title;
                    suggestions.appendChild(suggestion);
                });
                suggestions.style.display = 'block'; // Mostra as sugest√µes
            } else {
                suggestions.style.display = 'none'; // Esconde se n√£o houver resultados
            }
        } else {
            suggestions.style.display = 'none'; // Esconde quando o campo est√° vazio
        }
    });

    // Fecha as sugest√µes ao clicar fora
    document.addEventListener('click', (e) => {
        if (!e.target.closest('.search-container')) {
            suggestions.style.display = 'none';
        }
    });
</script>

        
        

            
        
